<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tvfood">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TVFOOD">
        <activity
            android:name=".app.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".app.CreateAccount"
            android:label="@string/app_name" />
        <activity
            android:name=".app.Food_List"
            android:label="@string/app_name" />
        <activity
            android:name=".app.Menu"
            android:label="@string/app_name" />
        <activity
            android:name=".app.Complete"
            android:label="@string/app_name" />
        <activity
            android:name=".app.Payment"
            android:label="@string/app_name" />
        <activity
            android:name=".app.Add_food"
            android:label="@string/app_name" />
        <activity
            android:name=".app.Admin_menu"
            android:label="@string/app_name" />


    </application>

</manifest>